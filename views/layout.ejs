<!doctype html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
        <title>Time keeper</title>
        <link rel="icon" type="image/x-icon" href="/images/favicon.ico">
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Italiana&display=swap" rel="stylesheet">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="/stylesheets/style.css">
        <% if (showAuthStyle) { %>
            <link rel="stylesheet" href="/stylesheets/authStyle.css">
        <% } %>
        <% if (showNavbarStyle) { %>
            <link rel="stylesheet" href="/stylesheets/navbarStyle.css">
        <% } %>
        <% if (showHomeStyle) { %>
            <link rel="stylesheet" href="/stylesheets/homeStyle.css">
        <% } %>
        <% if (showAdminUserStyle) { %>
            <link rel="stylesheet" href="/stylesheets/adminUserStyle.css">
        <% } %>
        <% if (showUserProfileStyle) { %>
            <link rel="stylesheet" href="/stylesheets/userProfile.css">
        <% } %>

    </head>
    <body>
        <% if (showNavbar) { %>
            <%- include('partials/navbar') %>
        <% } %>






        <%- body %>


        <!-- Optional JavaScript -->
        <!-- jQuery first, then Popper.js, then Bootstrap JS -->
        <!-- kompresovani jquery, a nama treba full verzija
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
        -->
        <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
        <% if (showUserScript) { %>
            <script type="text/javascript" src="/javascripts/editUser.js"></script>
        <% } %>
        <% if (showProjectScript) { %>
            <script type="text/javascript" src="/javascripts/editProject.js"></script>
        <% } %>
        <% if (showTasksScript) { %>
            <script type="text/javascript" src="/javascripts/addTask.js"></script>
        <% } %>
        <% if (showUserProjectScript) { %>
            <script type="text/javascript" src="/javascripts/addUserProject.js"></script>
            <script type="text/javascript" src="/javascripts/addUserTask.js"></script>
        <% } %>
        <% if (showUserEditScript) { %>
            <script type="text/javascript" src="/javascripts/editUserProfile.js"></script>
        <% } %>
        <script src="https://raw.githack.com/eKoopmans/html2pdf/master/dist/html2pdf.bundle.js"></script>
    </body>
</html>